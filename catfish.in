#!/usr/bin/env bash

APPNAME=catfish

if [ -e $APPNAME.pyc ]
    then python $APPNAME.pyc "$@"
    else
        if [ -e $APPNAME.py ]
            then python $APPNAME.py "$@"
            else
                cd %prefix%/share/$APPNAME
                if [ -e $APPNAME.pyc ]
                    then python $APPNAME.pyc "$@"
                    else
                        python $APPNAME.py "$@"
                fi
        fi
    fi
